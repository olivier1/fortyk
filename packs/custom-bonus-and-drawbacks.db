{"_id":"157Qsc0rMtmYNNWg","name":"Mastery of Gunnery","type":"talentntrait","img":"icons/svg/item-bag.svg","data":{"description":{"value":"<p>The Void-master can re-roll all failed Tests with Shooting Actions aboard a starship.</p>","type":"String"},"tier":{"value":"N/A","type":"Number"},"prereqs":{"value":"Void-Master","type":"String"},"aptitudes":{"value":"N/A","type":"String"},"specialisation":{"value":"N/A","type":"String"},"flagId":{"value":"MasteryofGunnery","type":"String"},"tags":{"value":"","type":"String"}},"effects":[],"folder":null,"sort":0,"permission":{"default":0,"jmEAwrSPS3Lww1Ye":3,"rH1O3GTgaMN2UUme":3},"flags":{"core":{"sourceId":"Item.uphlOKoVzBxayBPB"}}}
